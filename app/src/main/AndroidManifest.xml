<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aresliqourshop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AresLiqourShop">
        <activity android:name=".about"></activity>
        <activity android:name=".settings" />
        <activity android:name=".cart" />
        <activity android:name=".energy" />
        <activity android:name=".beer" />
        <activity android:name=".soda" />
        <activity android:name=".wines" />
        <activity android:name=".Alcohol" />
        <activity android:name=".DAshbord" />
        <activity
            android:name=".Login"
            android:screenOrientation="portrait" />
        <activity android:name=".Register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>